<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - AspDotNetCoreRazor</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/AspDotNetCoreRazor.styles.css" asp-append-version="true" />



    @* <link href="/Assets/Styles/css/Style.css" rel="stylesheet" />
    <script src="/Assets/Scripts/Script.js" type="text/javascript"></script>*@

    @*<link href="/Assets/libs/Jquery/ui/1.12.1/jquery-ui.css" rel="stylesheet" type="text/css" />*@
    @*<script type="text/javascript" src="/Assets/libs/Jquery/v3.2.1/jquery-3.2.1.js"></script>*@
    @*<script type="text/javascript" src="/Assets/libs/Jquery/popper.min.js"></script>*@
    @*<script type="text/javascript" src="/Assets/libs/Jquery/ui/1.12.1/jquery-ui.js"></script>*@

    @*<link href="/Assets/libs/Bootstrap/v4.0.0/dist/css/bootstrap.min.css" rel="stylesheet" />*@
    @*<script type="text/javascript" src="/Assets/libs/Bootstrap/v4.0.0/dist/js/bootstrap.min.js"></script>*@
    <link href="~/lib/sapgridview/v1.1.2/css/sapgridview.css" rel="stylesheet" />
    <link href="~/lib/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" />

    <link href="~/lib/datatables/1.10.20/DataTables-1.10.20/css/jquery.dataTables.min.css" rel="stylesheet" type="text/css" />
    <link href="~/lib/datatables/1.10.20/Buttons-1.6.1/css/buttons.dataTables.min.css" rel="stylesheet" type="text/css" />
    <link href="~/lib/datatables/1.10.20/ColReorder-1.5.2/css/colReorder.dataTables.min.css" rel="stylesheet" type="text/css" />
    <link href="~/lib/datatables/1.10.20/Responsive-2.2.3/css/responsive.dataTables.min.css" rel="stylesheet" type="text/css" />
    <link href="~/lib/datatables/1.10.20/FixedHeader-3.1.6/css/fixedHeader.dataTables.min.css" rel="stylesheet" type="text/css" />

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    <script src="~/lib/datatables/1.10.20/DataTables-1.10.20/js/jquery.dataTables.min.js"></script>
    <script src="~/lib/datatables/1.10.20/Buttons-1.6.1/js/dataTables.buttons.min.js"></script>
    <script src="~/lib/datatables/1.10.20/Buttons-1.6.1/js/buttons.flash.min.js"></script>
    <script src="~/lib/datatables/1.10.20/JSZip-2.5.0/jszip.min.js"></script>
    <script src="~/lib/datatables/1.10.20/pdfmake-0.1.36/pdfmake.min.js"></script>
    <script src="~/lib/datatables/1.10.20/pdfmake-0.1.36/vfs_fonts.js"></script>
    <script src="~/lib/datatables/1.10.20/Buttons-1.6.1/js/buttons.colVis.js"></script>
    <script src="~/lib/datatables/1.10.20/Buttons-1.6.1/js/buttons.html5.min.js"></script>
    <script src="~/lib/datatables/1.10.20/Buttons-1.6.1/js/buttons.print.min.js"></script>
    <script src="~/lib/datatables/1.10.20/ColReorder-1.5.2/js/dataTables.colReorder.min.js"></script>
    <script src="~/lib/datatables/1.10.20/Responsive-2.2.3/js/dataTables.responsive.min.js"></script>
    <script src="~/lib/datatables/1.10.20/FixedHeader-3.1.6/js/dataTables.fixedHeader.min.js"></script>
    <script src="~/lib/sapgridview/v1.1.2/js/sapgridview.js"></script>
    <script src="~/lib/jalalidate/jalali.js"></script>

    @await RenderSectionAsync("Scripts", required: false)
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container">
                <a class="navbar-brand" asp-area="" asp-page="/Index">AspDotNetCoreRazor</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="Home/Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/GridSamples/GridPage1">GridPage1</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2023 - AspDotNetCoreRazor - <a asp-area="" asp-page="/Privacy">Privacy</a>
        </div>
    </footer>

</body>
</html>